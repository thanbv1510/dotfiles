{
  "coc.preferences.formatOnSaveFiletypes": [
    "c",
    "go",
    "markdown",
    "typescript",
    "javascript",
    "json",
    "html",
    "css",
    "vue"
  ],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "filetypes": ["go"],
      "initializationOptions": {
        "gocodeCompletionEnabled": true,
        "diagnosticsEnabled": true,
        "lintTool": "golint"
      }
    }
  },
  "vetur.ignoreProjectWarning": true
}
